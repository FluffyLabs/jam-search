{
  "name": "jam-search-api",
  "version": "1.0.0",
  "description": "API for JAM Search",
  "main": "dist/index.js",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "build": "tsc",
    "lint": "biome check --write",
    "lint:ci": "biome ci",
    "start": "tsx ./src/index.ts",
    "dev": "tsx watch ./src/index.ts",
    "db:push": "drizzle-kit push --verbose --strict",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "graypaper:index-search": "tsx ./src/scripts/updateGraypaperData.ts",
    "update-graypapers": "tsx ./src/scripts/updateGraypapers.ts",
    "fill-archived-messages-for-n-days": "tsx ./src/scripts/fillArchivedMessageForNDays.ts",
    "generate-embeddings:batch": "tsx ./src/scripts/generateEmbeddingsBatch.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.1",
    "cheerio": "^1.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.42.0",
    "hono": "^4.7.7",
    "matrix-js-sdk": "^37.3.0",
    "node-schedule": "^2.1.1",
    "openai": "^4.97.0",
    "pdfjs-dist": "^5.2.133",
    "postgres": "^3.4.5",
    "tsx": "^4.19.3",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.14.1",
    "@types/node-schedule": "^2.1.0",
    "drizzle-kit": "^0.31.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  },
  "type": "module"
}
